{
  "name": "kabotan-e2e-tests",
  "version": "1.0.0",
  "description": "E2E tests for Kabotan Halloween features",
  "scripts": {
    "test:e2e:ui": "playwright test --project=ui-tests --reporter=list --quiet",
    "test:e2e:ui:headed": "playwright test --project=ui-tests --headed",
    "test:e2e:integration": "playwright test --project=integration-tests --reporter=list --quiet",
    "test:e2e:integration:headed": "playwright test --project=integration-tests --headed",
    "test:e2e:all": "playwright test --reporter=list --quiet",
    "test:e2e:debug": "playwright test --debug"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "dependencies": {
    "eventsource": "^4.0.0"
  }
}
